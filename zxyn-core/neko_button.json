  // Warna Teks

    "$default_text_color|default": [ 1.0, 1.0, 1.0 ],
    "$hover_text_color|default": [ 0.81, 0.98, 1.00 ],
    "$pressed_text_color|default": [ 0.00, 0.82, 0.92 ],
    "$locked_text_color|default": [ 1.00, 0.19, 0.29 ]
  }, 

