    "$pressed_button_texture|default": "textures/neko/borderless/pressed",
    "$locked_button_texture|default": "textures/neko/borderless/pressed_no_hover",

    "$default_text_color|default": [ 0.0, 0.0, 0.0 ],
    "$hover_text_color|default": [ 0.15, 0.15, 0.15 ],
    "$pressed_text_color|default": [ 1.00, 1.00, 1.00 ],
    "$locked_text_color|default": [ 1.00, 0.19, 0.29 ]
