  "alpha_0": {
    "anim_type": "alpha",
    "easing": "linear",
    "duration": 0.3,
    "from": 1,
    "to": 0.8,
    "next": "@zxyn.alpha_1"
  },

  "alpha_1": {
    "anim_type": "alpha",
    "easing": "linear",
    "duration": 1.2,
    "from": 0.8,
    "to": 1,
    "next": "@zxyn.alpha_0"
  },


  "han_uwu": {
    "type": "panel",
    "layer": 5,
    "controls": [
      {
        "glyph": {
          "type": "image",
          "anchor_from": "left_middle",
          "blinear": true,
          "anchor_to": "left_middle",
          "offset": "$glyph_offset",
          "texture": "$quantum_a",
          "size": [ 21, 21 ],
          "layer": 15,
          "color": "$warna_icon",
          "variables": [
            {
              "requires": "$default_state",
              "$glyph_offset": [ 2 , 0.5 ],
              "$warna_icon":  [ 1.0, 1.0, 1.0 ]
            },
            {
              "requires": "$hover_state",
              "$glyph_offset": [ 2 , 0.5 ],
              "$warna_icon": [ 0.80, 0.95, 1.00 ]
            },
            {
              "requires": "$pressed_state",
              "$glyph_offset": [ 2 , 0.5 ],
              "$warna_icon": [ 0.00, 0.85, 1.00  ]
            },
            {
              "requires": "$locked_state",
              "$glyph_offset":[ 2 , 0.5 ],
              "$warna_icon": "white"
            }
          ],
          "alpha": "$content_alpha",

          "$glyph_bindings|default": [],
          "bindings": "$glyph_bindings"
        }
      },
      {
        "title": {
          "layer": 11,
          "type": "label",
          "font_type": "EudoxusSans-Bold",
          "font_scale_factor": 1.6,
          "alpha": "@zxyn.alpha_0",
          "anchor_from": "left_middle",
          "anchor_to": "left_middle",
          "color": "$text_color",
          "text_alignment": "left",
          "offset": [ 27 , -6 ],
          "text": "$button_text"
        }
      },
      {
        "title": {
          "layer": 11,
          "type": "label",
          "font_type": "EudoxusSans-Bold",
          "font_scale_factor": 0.55,
          "anchor_from": "left_middle",
          "anchor_to": "left_middle",
          "color": "$text_color",
          "text_alignment": "left",
          "offset": [ 27 , 6.85 ],
          "text": "SERVERS | REALMS | WORLD"
        }
      }
    ]
  },

  "start_button@common_buttons.light_text_button": {
    "size": [ "100% - 5px", "100% - 5px" ],
    "$button_type_panel": "zxyn.han_uwu",
    "$quantum_a": "textures/neko/button_icon/start_icon",
    "$default_text_color|default": [1.0, 1.0, 1.0 ],
    "$hover_text_color|default":  [ 0.80, 0.95, 1.00 ],
    "$pressed_text_color|default":  [ 0.80, 0.95, 1.00 ],
    "$locked_text_color|default": [ 0.0, 0.0, 0.0 ],
    "$border_visible": false,

    "$light_border_default_color": [ 1.0, 1.0, 1.0 ],
    "$light_border_hover_color": [ 1.0, 1.0, 1.0 ],
    "$light_border_pressed_color": [ 1.0, 1.0, 1.0 ],
    "$light_border_locked_color": [ 0.05, 0.05, 0.05 ],
    "$pressed_button_name": "$play_button_target",
    "$default_button_texture|default": "",
    "$hover_button_texture|default": "",
    "$pressed_button_texture|default": "",
    "$locked_button_texture|default": "",
    "$button_text": "Play"
  }
