                      ]
                    }
                  }
                ]
              }
            },
            {
              "spacer": { "type": "panel", "size": [ "1%", 1 ]}
            }
          ]
        }
      }
    ]
  },


  "xbox_panel@neko_c.bg_3": {
    "layer": 2,
    "alpha": 0.35,
    "color": [ 0.08, 0.08, 0.08 ],
    "size": [  "100%" , "100% - 5px" ],

    "anchor_from": "left_middle",
    "anchor_to": "left_middle",
    "$warna_text": [ 1.0, 1.0, 1.0 ], //HITAM

    "$panel_img_sign_size": [ 15 , "100%" ],
    "$panel_img_not_sign_size": [ 15 , "100%" ],

    "$panel_text_sign_size": [ "fill" , "100%" ],
    "$panel_text__not_sign_size": [ "fill" , "100%" ],

    "controls": [
      {
        "xbox_sign_in": {
          "type": "stack_panel",
          "orientation": "horizontal",
          "size": [ "100%" , "100%" ],
          "controls": [
            {
              "spasi": {
                "type": "panel",
                "size": [ 3, 3 ]
              }
            },
            {
              "panel_img_sign": {
                "type": "panel",
                "size": "$panel_img_sign_size",
                "bindings": [
                  {
                    "binding_name": "#sign_in_visible",
                    "binding_name_override": "#visible" 
                  }
                ],
                "controls": [
                  {
                    "image": {
                      "type": "image",
                      "layer": 11,
                      "anchor_from": "center",
                      "anchor_to": "center",
                      "size": [ 15, 15 ],
                      "texture": "textures/neko/main_icon/xbox_b"
                    }
                  }
                ]
              }
            },
            {
              "panel_img_not_sign": {
                "type": "panel",
                "size": "$panel_img_not_sign_size",
                "bindings": [
                  {
                    "binding_name": "#feedback_visible",
                    "binding_name_override": "#visible" 
                  }
                ],
                "controls": [
                  {
                    "image": {
                      "type": "image",
                      "layer": 11,
                      "anchor_from": "center",
                      "anchor_to": "center",
                      "size": [ 15, 15 ],
                      "texture": "textures/neko/main_icon/xbox_a"
                    }
                  }
                ]
              }
            },
            {
              "spasi": {
                "type": "panel",
                "size": [ 5, 5 ]
              }
            },
            {
              "panel_text_sign": {
                "type": "panel",
                "size": "$panel_text_sign_size",
                "controls": [
                  {
                    "label": {
                      "type": "label",
                      "layer": 11,
                      "size": [ "100%", "default" ],
                      "alpha": 1,  //"@zxyn_main_ui.alpha_0",
                      "font_scale_factor": "$ukuran_font",
                      "font_type": "$gaya_font",
                      "anchor_from": "left_middle",
                      "anchor_to": "left_middle",
                      "color": "$warna_text",
                      "text": "xbox.sign_text_true",
                      "bindings": [
                        {
                          "binding_name": "#feedback_visible",
                          "binding_name_override": "#visible" 
                        }
                      ]
                    }
                  },
                  {
                    "label": {
                      "type": "label",
                      "layer": 11,
                      "size": [ "100%", "default" ],
                      "font_scale_factor": "$ukuran_font",
                      "font_type": "$gaya_font",
                      "anchor_from": "left_middle",
                      "anchor_to": "left_middle",
                      "color": "$warna_text",
                      "text": "xbox.sign_text_false",
                      "bindings": [
                        {
                          "binding_name": "#sign_in_visible",
                          "binding_name_override": "#visible" 
                        }
                      ]
                    }
                  }
                ]
              }
            }
          ]
        }
      }
    ]
