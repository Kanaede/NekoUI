                "type": "panel",
                "size": [ "fill", "100%" ],
                "controls": [
                  {
                    "top_stack": {
                      "type": "stack_panel",
                      "orientation": "horizontal",
                      "anchor_from": "top_right",
                      "anchor_to": "top_right",
